<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://www.geak.ch/Import/v4/" elementFormDefault="qualified" targetNamespace="http://www.geak.ch/Import/v4/" xmlns:xs="http://www.w3.org/2001/XMLSchema" version="6.0.0">
  <xs:element name="SIARecord" type="xsQuestionnaireRecord" />
  <xs:complexType name="xsQuestionnaireRecord">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="Version" type="xsString50">
        <xs:annotation>
          <xs:documentation>File format version</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="General" type="xsGeneral">
        <xs:annotation>
          <xs:documentation>The general part contains general information about the building's owner and the building itself</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="CurrentState" type="xsCurrentState" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="xsGeneral">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="OwnerAddress" type="xsOwnerAddress">
        <xs:annotation>
          <xs:documentation>Contact information of the building owner</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="BuildingData" type="xsBuildingData">
        <xs:annotation>
          <xs:documentation>General information about building itself</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="BuildingUsages" type="xsBuildingUsagesType"/>
    </xs:sequence>
    <xs:attribute name="ProjectName" type="xsString50">
      <xs:annotation>
        <xs:documentation>A short handle to identify the project in the portfolio, e.g. a project number or a reference</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="xsOwnerAddress">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="Title" type="xsTitles">
        <xs:annotation>
          <xs:documentation>Title of the owner (enumeration)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Name1" type="xsString50">
        <xs:annotation>
          <xs:documentation>First name part of the owner (e.g. first name or name of company)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Name2" type="xsString50">
        <xs:annotation>
          <xs:documentation>Second name part of the owner (e.g. last name or c/o)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Address" type="xsString250">
        <xs:annotation>
          <xs:documentation>Postal address (street and housenumber) of the owner</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="POBox" type="xsString250">
        <xs:annotation>
          <xs:documentation>Post office box of the owner (if specified)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Zip" type="xs:int">
        <xs:annotation>
          <xs:documentation>Postcode/zip code of the owner</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="City" type="xsString50">
        <xs:annotation>
          <xs:documentation>City of the owner</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Country" type="xsString100">
        <xs:annotation>
          <xs:documentation>Country code of the owner</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="EMail" type="xsEmail">
        <xs:annotation>
          <xs:documentation>E-mail address of the owner</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Phone1" type="xsString100">
        <xs:annotation>
          <xs:documentation>Primary telephone number of the owner</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Phone2" type="xsString100">
        <xs:annotation>
          <xs:documentation>Secondary telephone number of the owner</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="xsTitles">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Ms" />
      <xs:enumeration value="Mr" />
      <xs:enumeration value="MrAndMs" />
      <xs:enumeration value="Company" />
      <xs:enumeration value="Family" />
      <xs:enumeration value="HomeOwner" />
      <xs:enumeration value="CommunityAssociation" />
      <xs:enumeration value="PropertyManagement" />
      <xs:enumeration value="MunicipalAdministration" />
      <xs:enumeration value="" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="xsBuildingData">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="Zip" type="xs:int">
        <xs:annotation>
          <xs:documentation>Swiss postcode/zip code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="City" type="xsString250">
        <xs:annotation>
          <xs:documentation>City</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Community" type="xsString250">
        <xs:annotation>
          <xs:documentation>Name of the community</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="StreetName" type="xsString250">
        <xs:annotation>
          <xs:documentation>Name of the street</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="HouseNumber" type="xsString250">
        <xs:annotation>
          <xs:documentation>Housenumber or empty string if not specified</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="BuildingName" type="xsString250">
        <xs:annotation>
          <xs:documentation>Name of building (e.g. Haus zum Schwan)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="WeatherStation" type="xsString250">
        <xs:annotation>
          <xs:documentation>Name of the assigned weather station</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="EGID" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: EGID is ignored during the import.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="YearOfConstruction" type="xs:int">
        <xs:annotation>
          <xs:documentation>Year of building construction</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="PlotNumber" type="xsString250">
        <xs:annotation>
          <xs:documentation>Number of the plot or parcel</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="BuildingCategory" type="xsBuildingCategories">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Category of the building (enumeration)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" default="0" name="PeopleCount" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Number of people residing inside the building</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ApartmentsCount" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Number of appartements in total (check sum)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="RoomCountPerApartment" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Average number of rooms per appartements</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ApartmentsCount1Rooms" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Number of appartements with 1 room</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ApartmentsCount2Rooms" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Number of appartements with 2 rooms</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ApartmentsCount3Rooms" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Number of appartements with 3 rooms</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ApartmentsCount4Rooms" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Number of appartements with 4 rooms</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ApartmentsCount5Rooms" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Number of appartements with 5 rooms</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ApartmentsCount6Rooms" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Number of appartements with 6 rooms</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ApartmentsCountOver6Rooms" type="xs:int">
        <xs:annotation>
          <xs:documentation>Deprecated: Use BuildingUsages instead. Number of appartements with 7 or more rooms</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="EnergyReferenceArea" type="xs:int">
        <xs:annotation>
          <xs:documentation>Energy reference area in square meters</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="AverageRoomHeight" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>Average height of the rooms</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="FullFloorsCount" type="xs:int">
        <xs:annotation>
          <xs:documentation>Number of floors</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="BuildingDepth" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>Extend of the building</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="GroundPlanType" type="xsGroundPlanTypes">
        <xs:annotation>
          <xs:documentation>Type of the ground plan (enumeration)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="ConstructionType" type="xsConstructionTypes">
        <xs:annotation>
          <xs:documentation>Type of the construction (enumeration)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="BestKnownWeatherStation" type="xsString250">
        <xs:annotation>
          <xs:documentation>Name of the assigned weather station, whose climate values are the best match for the building.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Altitude" type="xs:int">
        <xs:annotation>
          <xs:documentation>Altitude of the Building. Overrides the altitude of BestKnownWeatherStation</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="xsBuildingCategories">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ApartmentHouse" />
      <xs:enumeration value="SingleHome" />
      <xs:enumeration value="Administration" />
      <xs:enumeration value="School" />
      <xs:enumeration value="Shop" />
      <xs:enumeration value="Restaurant" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="xsFacadeStructures">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Flat" />
      <xs:enumeration value="NormallyStructured" />
      <xs:enumeration value="HeavilyStructured" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="xsGroundPlanTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Compact" />
      <xs:enumeration value="Elongated" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="xsConstructionTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Heavy" />
      <xs:enumeration value="Middle" />
      <xs:enumeration value="Light" />
      <xs:enumeration value="VeryLight" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="xsBuildingPartCurrentState">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="Condition" type="xsConditionType">
        <xs:annotation>
          <xs:documentation>Condition of the component/ building part (enumeration)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Description" type="xsString4000">
        <xs:annotation>
          <xs:documentation>Textual description of the condition (free text)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Renewals" type="xsString4000">
        <xs:annotation>
          <xs:documentation>Textual description of the renewals (free text)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="xsBuildingPartCurrentStateConditionOptional">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="Condition" type="xsConditionType">
        <xs:annotation>
          <xs:documentation>Condition of the component/ building part (enumeration)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Description" type="xsString4000">
        <xs:annotation>
          <xs:documentation>Textual description of the condition (free text)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Renewals" type="xsString4000">
        <xs:annotation>
          <xs:documentation>Textual description of the renewals (free text)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="xsCurrentState">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="Remarks" type="xsString1000">
        <xs:annotation>
          <xs:documentation>Overall remarks to the building (free text)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="BuildingEnvelope" type="xsBuildingEnvelopeType">
        <xs:annotation>
          <xs:documentation>Specifies the building envelope in detail</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="xsBuildingEnvelopeType">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="Description" type="xsString4000">
        <xs:annotation>
          <xs:documentation>Condition of the building envelope (free text)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="RoofsAndCeilings" type="xsRoofsAndCeilingsType">
        <xs:annotation>
          <xs:documentation>All types of roofs and ceilings</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Walls" type="xsWallsType">
        <xs:annotation>
          <xs:documentation>All types of walls</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="WindowsAndDoors" type="xsWallOpeningsType">
        <xs:annotation>
          <xs:documentation>All windows and doors</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Floors" type="xsFloorsType">
        <xs:annotation>
          <xs:documentation>All types of floors</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ThermalBridges" type="xsThermalBridgesType">
        <xs:annotation>
          <xs:documentation>All types of thermal bridges</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="xsRoofsAndCeilingsType">
    <xs:complexContent mixed="false">
      <xs:extension base="xsBuildingPartCurrentState">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="GeneralRoof" type="xsGeneralRoofType" />
          <xs:element minOccurs="0" maxOccurs="1" name="ConditionOtherCeilings" type="xsConditionType">
            <xs:annotation>
              <xs:documentation>Condition of the component / building part (enumeration)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="DescriptionOtherCeilings" type="xsString4000">
            <xs:annotation>
              <xs:documentation>Textual description of the condition (free text)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="RenewalsOtherCeilings" type="xsString4000">
            <xs:annotation>
              <xs:documentation>Textual description of the renewals (free text)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="Items">
            <xs:complexType>
              <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element minOccurs="1" maxOccurs="1" name="Roof" nillable="true" type="xsRoof" />
                <xs:element minOccurs="1" maxOccurs="1" name="Ceiling" nillable="true" type="xsCeiling" />
              </xs:choice>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="xsRoof">
    <xs:complexContent mixed="false">
      <xs:extension base="xsInnerSurfaceWithOrientation">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="Type" type="xsRoofType" />
          <xs:element minOccurs="0" maxOccurs="1" default="false" name="HeatingElement" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Flag that specifies whether a wall heating is built into this component</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="xsRoofType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PitchedRoof" />
      <xs:enumeration value="FlatRoof" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="xsGeneralRoofType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="FlatRoof" />
      <xs:enumeration value="PitchedRoofNotHeated" />
      <xs:enumeration value="PitchedRoofPartiallyHeated" />
      <xs:enumeration value="PitchedRoofFullyHeated" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="xsCalculationBaseType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PerItem" />
      <xs:enumeration value="PerSquareMeter" />
      <xs:enumeration value="PerMeter" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="xsMeasureType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NoMeasure" />
      <xs:enumeration value="Replacement" />
      <xs:enumeration value="GlassReplacement" />
      <xs:enumeration value="OutsideInsulation" />
      <xs:enumeration value="Cavity" />
      <xs:enumeration value="InsideInsulation" />
      <xs:enumeration value="BackVentilated" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="xsBuildingEnvelopeEntity" abstract="true">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="ExpertCode" type="xsExpertCode">
        <xs:annotation>
          <xs:documentation>Custom code/short handle of the component</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ExpertDescription" type="xsString250">
        <xs:annotation>
          <xs:documentation>Short textual description of the component</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Quantity" type="xs:int">
        <xs:annotation>
          <xs:documentation>Number of identical components in the same building</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="InvestmentCosts" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>The investment costs for the (new) component/ building</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="CostsCalculationBase" type="xsCalculationBaseType">
        <xs:annotation>
          <xs:documentation>The calculation basis for the investment costs.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="MaintenanceCostsPerYear" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>The maintenance costs per year for the (new) component/ building</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="LifeSpan" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>The life span of the (new) component/ building</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="MeasureType" type="xsMeasureType">
        <xs:annotation>
          <xs:documentation>The type of action for the (new) component</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="xsSurface" abstract="true">
    <xs:complexContent mixed="false">
      <xs:extension base="xsBuildingEnvelopeEntity">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="Area" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>Surface area of the component in square meters</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="UValue" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>U-value of the component in W/(m2*K)</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="xsFloorExterior">
    <xs:complexContent mixed="false">
      <xs:extension base="xsFloorBase" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="xsWallOpening">
    <xs:complexContent mixed="false">
      <xs:extension base="xsSurface">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="GValue" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>G-value of the component</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="FF" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>Reduction due to frame of the component</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="FS" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>Reduction due to shadowing (fs1 * fs2 * fs3) of the component</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" default="1.0" name="BFactor" type="xsBFactorType">
            <xs:annotation>
              <xs:documentation>Reduction (b-Factor) of the component</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="Orientation" type="xsOrientationsExtended">
            <xs:annotation>
